{
    "presets": [
        ["env",{
            "modules": false
        }],
        "stage-2"
    ],
    "plugins": [
        "transform-runtime",
        [ "component",[{
                "libraryName": "element-ui",
                "styleLibraryName": "~theme"
            }]
        ],
        "dynamic-import-node"
    ],
    "env": {
        "test": {
            "presets": [ "env","stage-2"],
            "plugins": ["istanbul"]
        }
    }
}
